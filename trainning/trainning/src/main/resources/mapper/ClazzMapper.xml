<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.jad.trainning.mapper.ClazzMapper">


    <select id="findAll"  resultType="clazz">
        select * from clazz
    </select>

    <insert id="save" parameterType="clazz">
        insert into clazz values (#{id},#{name})
    </insert>

    <select id="findById" parameterType="string" resultType="clazz">
        select * from clazz where id=#{id}
    </select>

    <update id="update" parameterType="clazz">
        update clazz set name=#{name} where id=#{id}
    </update>

    <delete id="delete" parameterType="string">
        delete from clazz where id=#{id}
    </delete>
</mapper>