<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dao.ProvinceDao">

    <select id="findAll"  resultType="com.example.pojo.Province">
        select * from province
    </select>

    <select id="findById" resultType="com.example.pojo.Province">
        select *
        from province where id=#{id};
    </select>

    <select id="findByName" resultType="com.example.pojo.Province">
        select * from province where `name`=#{name}
    </select>

    <insert id="save" parameterType="com.example.pojo.Province">
        insert into province values(#{id},#{name})
    </insert>
</mapper>