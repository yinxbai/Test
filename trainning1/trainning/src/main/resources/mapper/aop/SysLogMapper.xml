<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.jad.trainning.aop.mapper.SysLogMapper">

    <select id="findAll" resultType="hashMap" >
        select * from sys_log order by create_time desc
    </select>

    <insert id="save" parameterType="sysLog">
        insert into sys_log(id,account,ip,url,method,create_time)
        values (#{id},#{account},#{ip},#{url},#{method},#{createTime})
    </insert>

    <select id="queryList" resultType="syslog" >
        select id,account,ip,url,method,create_time createTime from sys_log order by create_time desc
    </select>
</mapper>